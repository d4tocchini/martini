= meta_tags c(:site, :title), :description => c(:site, :tagline)
= meta_tag "google-analytics", ""

/ stylesheets
= stylesheet_link_tag "reset", "general", "martini", "wmd", "application"
- %w(jquery/jquery.prettyPhoto-min).each do |stylesheet|
  = stylesheet_link_tag "http://cachedcommons.org/stylesheets/#{stylesheet}.css"
/[if lte IE 7]
  = stylesheet_link_tag "ie", :media => "all"
/[if lte IE 6]
  = stylesheet_link_tag "ie6", :media => "all"
/ javascripts
/[if lt IE 7]
  = javascript_include_tag "http://cachedcommons.org/javascripts/ie/DD_belatedPNG-min.js"    
- %w(jquery/jquery-1.4.2 jquery/jquery.google-analytics-1.1.3 jquery/jquery.system-0.1.1 jquery/jquery.hoverIntent jquery/jquery.cycle.all-2.8.6 jquery/jquery.prettyPhoto).each do |javascript|
  = javascript_include_tag "http://cachedcommons.org/javascripts/#{javascript}-min.js"
= javascript_include_tag "cufon-yui", "Vegur_300-Vegur_700.font", "superfish", "extra"
- %w(text/showdown).each do |javascript|
  = javascript_include_tag "http://cachedcommons.org/javascripts/#{javascript}-min.js"
= javascript_include_tag "jquery-ui", "jquery.tablesorter-min", "application"